{
  "containerDefinitions": [
    {
      "name": "dc-main",
      "image": "public.ecr.aws/v0g8b0z8/dc-main:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "dc-main-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:PORT::"
        },
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:NODE_ENV::"
        },
        {
          "name": "RDS_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:RDS_HOST::"
        },
        {
          "name": "RDS_PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:RDS_PORT::"
        },
        {
          "name": "RDS_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:RDS_USERNAME::"
        },
        {
          "name": "RDS_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:RDS_PASSWORD::"
        },
        {
          "name": "RDS_DATABASE",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:RDS_DATABASE::"
        },
        {
          "name": "KAKAO_REST_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:KAKAO_REST_API_KEY::"
        },
        {
          "name": "KAKAO_REDIRECT_URI",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:KAKAO_REDIRECT_URI::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:JWT_SECRET::"
        },
        {
          "name": "FRONT_END_URI",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:561166824006:secret:dc-env-0ClPYi:FRONT_END_URI::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/dc-td-main",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "dc-td-main",
  "executionRoleArn": "arn:aws:iam::561166824006:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
